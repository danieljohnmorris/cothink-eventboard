<%= week %>
<li class="event" id="event-id-<%=CGI.escape(event.title)%>">
  <%= calendar_page %>
  <div class="nontime">
    <h4>
		<a class="star" href="#star">
			<img src="/images/star_dim.gif" alt="star" />
			<span class="starred-count">34</span>
		</a>
		<%=link_to event.title, event_path(event) %>
	</h4>
    <p class="data">
      <!-- <span class="time"><%= event.start_time %><span class="ampm"><%= event.start_ampm %></span></span> at -->
      <span><%= event.location %> (<%= map_link(event.location) %>)</span>,

      <% if event.organisation %>
		<span>organised by <%= link_to event.organisation.name, organisation_path(event.organisation) %></span>
	  <% end %>
    </p>
  </div>
</li>